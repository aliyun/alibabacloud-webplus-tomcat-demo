    <div class="pb_gradient_v1 pb_height-110"></div>

    <section class="bg-light pb_slant-white pb_pt-60 pb_pb-250" id="section-envs">
        <div class="container">
            <h3>{{#i18n}}env.title{{/i18n}}</h3>
            <p>{{#i18n}}env.desc [{{envsDocUrl}}]{{/i18n}}</p>
            <table class="table table-striped pb_font-13">
                <thead>
                    <tr>
                        <th scope="col" class="w-20">{{#i18n}}var.name{{/i18n}}</th>
                        <th scope="col" class="w-50">{{#i18n}}var.value{{/i18n}}</th>
                        <th scope="col" class="w-30">{{#i18n}}var.desc{{/i18n}}</th>
                    </tr>
                </thead>

                <tbody>
                    {{#envs}}
                    <tr>
                        <td>{{key}}</td>
                        <td>{{value}}</td>
                        <td>{{#i18n}}env.{{key}}.desc{{/i18n}}</td>
                    </tr>
                    {{/envs}}
                </tbody>
            </table>
        </div>
    </section>

    <footer class="pb_footer pb_white" id="section-footer">
        <div class="container">
            <div class="row">
                <div class="col text-center">
                    {{^intl}}
                        <p class="pb_pt-50">
                            <img src="data:image/svg+xml;base64," class="dingtalk">&nbsp;{{#i18n}}scan.qrcode{{/i18n}}
                        </p>
                        <img src="/assets/images/dingtalk-qrcode.png?2512611a" class="pb-5 pb_w-30" />
                        <p class="pb_font-14">
                            © 2017 <a href="https://uicookies.com/">Landing</a> Free Bootstrap4. All Rights
                            Reserved. <br> Designed &amp; Developed by <a href="https://uicookies.com/">uicookies.com</a>.
                        </p>
                    {{/intl}}
                    {{#intl}}
                        <p class="pb_font-14 pb_pt-50">
                            © 2017 <a href="https://uicookies.com/">Landing</a> Free Bootstrap4. All Rights
                            Reserved. <br> Designed &amp; Developed by <a href="https://uicookies.com/">uicookies.com</a>.
                        </p>
                    {{/intl}}
                </div>
            </div>
        </div>
    </footer>

    <div id="pb_loader" class="fullscreen">
        <svg class="circular" width="48px" height="48px">
            <circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"></circle>
            <circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10"
                    stroke="#1d82ff">
            </circle>
        </svg>
    </div>

    <script type="text/javascript" src="home/bundle.js"></script>

</body>
</html>
